#%Module1.0
proc ModulesHelp { } {
    global product version
    puts stderr "This module adds $product/$version to your environment."
}
# Define variables
#
set product sdss_install
set version master
conflict $product
#
# module-whatis
#
module-whatis "Sets up $product/$version in your environment."
#
# Set environment
#
set PRODUCT_DIR ~/software/sdss/$product/$version
setenv [string toupper $product]_DIR $PRODUCT_DIR
setenv [string toupper $product]_GRAPHQL_DIR $PRODUCT_DIR/graphql
setenv [string toupper $product]_KEY GITHUB_PERSONAL_ACCESS_TOKEN
setenv [string toupper $product]_LONGPATH False


prepend-path PATH $PRODUCT_DIR/bin
prepend-path PYTHONPATH $PRODUCT_DIR/python

